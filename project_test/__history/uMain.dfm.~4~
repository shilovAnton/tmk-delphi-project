  object ADOQuery1: TADOQuery
    Active = True
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT f.name AS firm_name, c.name AS city_jur, c2.name AS city_' +
        'post'
      
        'FROM (FIRM AS f INNER JOIN city AS c ON c.city_id = F.JUR_CITY_I' +
        'D) LEFT JOIN city AS c2 ON c2.city_id = F.POST_CITY_ID'
      
        'WHERE Instr(LCase(f.name),LCase('#39#1075#39'))>0 And (Instr(LCase(c.name)' +
        ',LCase('#39#1084#1086#39'))>0 Or Instr(LCase(c2.name),LCase('#39#39'))>0)'
      'ORDER BY f.name;')
    Left = 584
    Top = 168
